{
  "name": "open-orchestra/open-orchestra-media-demo",
  "license": "MIT",
  "type": "project",
  "description": "Example of OpenOrchestra external media domain implementation",
  "autoload": {
    "psr-0": {
      "": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "incenteev/composer-parameter-handler": "~2.0",
    "open-orchestra/open-orchestra-base-bundle": "~1.0.0",
    "open-orchestra/open-orchestra-media-bundle": "~1.0.0",
    "solution/mongo-odm-aggregation-bundle": "*",
    "doctrine/cache": "1.5.*",
    "doctrine/common": "2.5.1"
  },
  "require-dev": {
    "scrutinizer/ocular": "1.1.1",
    "jmikola/geojson": "~1.0",
    "phpunit/phpunit" : "4.7.7",
    "phake/phake": "2.1.0"
  },
  "scripts": {
    "post-install-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
    ],
    "post-update-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
    ]
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "symfony-app-dir": "app",
    "symfony-web-dir": "web",
    "incenteev-parameters": {
      "file": "app/config/parameters.yml"
    }
  }
}
